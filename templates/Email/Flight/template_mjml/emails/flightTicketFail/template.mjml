  <mj-body>
    <mj-section>
      <mj-column>
        <mj-raw>
          <section class="bg-navicu container p-3">
            <a href="https://www.navicu.com" target="_blank">
							<img src="https://www.navicu.com/es/assets/img/Home/header/logo-navicu.png" width="200px"/>
            </a>
          </section>
        </mj-raw>
      	<mj-raw>
          <section class="mb-4">
            <div class="container bg-yellow lato-heavy head px-3 pt-2">
              <div class="flex-start" style="width: 100%">
                <div class="mr-2 mb-2" style="flex-basis: 5%; height: 26px;">
                	<img width="26px" src="time.svg"/>
                </div>
                <div class="flex-between flex-wrap" style="flex-basis: 95%">
                  <p class="fc-yellow mb-2">Compra en proceso de verificación</p>
                  <p class="lato-light fc-yellow mb-2">Localizador 
                    <span class="lato-heavy">5CB5BB1A</span>
                  </p>
                </div>
              </div>
          	</div>
          </section>
        </mj-raw>
        <mj-raw>
          <section>
            <div class="content mx-2 py-4 bg-light-yellow">
              <div class="flex-start px-4" style="align-items: start">
               	<div style="width: 35px; margin-top: 40px;">
