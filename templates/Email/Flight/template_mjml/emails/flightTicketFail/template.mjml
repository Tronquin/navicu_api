.pb-4,.py-4{padding-bottom:1.5rem!important}.pl-4,.px-4{padding-left:1.5rem!important}.p-5{padding:3rem!important}.pt-5,.py-5{padding-top:3rem!important}.pr-5,.px-5{padding-right:3rem!important}.pb-5,.py-5{padding-bottom:3rem!important}.pl-5,.px-5{padding-left:3rem!important}.mx-auto{margin-left:auto!important; margin-right:auto!important}
      
      .head {
      	display: flex;
      	flex-wrap: wrap;
      	justify-content: space-between;
      	align-items: center;
      }
      
      .content {
      	border-radius: .5rem;
      }
      
