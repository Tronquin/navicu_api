                      <td width="96%" align="left" class="text-cel2-col1" style="font-size: 14px; font-family: lato; font-weight: normal; color: #54284E;">
                          Detalles del vuelo    
                      </td>
                    </tr>                      
                  </tbody>
                </table><table bgcolor="#ffffff" border="0" cellpadding="0" cellspacing="0" width="100%">
                  <tbody>
                    {% for flight in flights %}
                     {% if flight['return'] or flight['isReturn'] %}
                    <tr height="35px">
                       <td width="4%">
                       <span></span>
                      </td>
                      
                      <td width="30%" bgcolor="#F0F0F0" align="left" class="text-cel2-col1" style="font-size: 14px; font-family: lato; font-weight: normal; color: #54284E;">
                            <div class="margin-left" style="margin-left: 15px;">
                            <span class="text-td-1" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #54284E;">Vuelta </span>
                            <span class="text-td-2" style="font-size: 14px; font-family: lato; font-weight: bold; color: #54284E;">{{ flight['departure']|date('d-m-Y H:i') }} </span>
                            <span class="text-td-2" style="font-size: 14px; font-family: lato; font-weight: bold; color: #54284E;">{{ flight['departure']|date('d-m-Y H:i') }} </span> 
                          </div>
                      </td>
                      <td bgcolor="#F0F0F0" width="32" align="left" class="text-cel2-col1" style="font-size: 14px; font-family: lato; font-weight: normal; color: #54284E;">
                            
                      </td>
                      <td bgcolor="#F0F0F0" width="30" align="left" class="text-cel2-col1" style="font-size: 14px; font-family: lato; font-weight: normal; color: #54284E;">
                            
                      </td>
                      <td width="4%">
                       <span></span>
                      </td>
                    </tr>
                     {% else %}
                      <tr height="35px">
                         <td width="4%">
                         <span></span>
                        </td>
                      
                          <td width="30%" bgcolor="#F0F0F0" align="left" class="text-cel2-col1" style="font-size: 14px; font-family: lato; font-weight: normal; color: #54284E;">
                                <div class="margin-left" style="margin-left: 15px;">
                                <span class="text-td-1" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #54284E;">Ida </span>
                                <span class="text-td-2" style="font-size: 14px; font-family: lato; font-weight: bold; color: #54284E;">{{ flight['departure']|date('d-m-Y H:i') }} </span>
                                <span class="text-td-2" style="font-size: 14px; font-family: lato; font-weight: bold; color: #54284E;">{{ flight['departure']|date('d-m-Y H:i') }} </span> 
                              </div>
                          </td>
                          <td bgcolor="#F0F0F0" width="32" align="left" class="text-cel2-col1" style="font-size: 14px; font-family: lato; font-weight: normal; color: #54284E;">
                                
                          </td>
                          <td bgcolor="#F0F0F0" width="30" align="left" class="text-cel2-col1" style="font-size: 14px; font-family: lato; font-weight: normal; color: #54284E;">
                                
                          </td>
                          <td width="4%">
                           <span></span>
                          </td>
                      </tr>
                     {% endif %}

                    <tr height="35px">
                       <td width="4%">
                       <span></span>
                      </td>
                      
                      <td width="30%" align="left" class="text-cel2-col1 " style="font-size: 14px; font-family: lato; font-weight: normal; color: #54284E;">
                         <div class="margin-left" style="margin-left: 15px;">
                          <span class="text-details-col1" style="font-size: 14px; font-family: lato; font-weight: normal; color: #808080;">{{ flight['departure']|date('d-m-Y H:i') }} <br>
                          {{ flight['originCode'] ~ " " ~ flight['originName'] }}</span>
                        </div>
                      </td>
                      <td width="44%" align="left" class="text-cel2-col1" style="font-size: 14px; font-family: lato; font-weight: normal; color: #54284E;">
                         
                         <span class="text-details-col1" style="font-size: 14px; font-family: lato; font-weight: normal; color: #808080;">{{ flight['arrival']|date('d-m-Y H:i') }}<br>
                         {{ flight['destinationCode'] ~ " " ~ flight['destinationName'] }}</span>
                         
                      </td>
                      <td width="18%" align="left" class="text-regular-14" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #808080;">
                        <table>
                          <tbody>
                            <tr>
                              <td>
                                 {% if flight['logo_exists'] %}
                                <img class="align-aerolin-vuelo " align="left" width="50px" src="http://navicu.com/images/airlines/{{ flight['airlineCode'] }}.png" style="margin-right: 25px;">
                                 {% endif %}
                              </td>
                            </tr>
                             <tr>
                              <td>
                                <div class="margin-right" style="margin-right: 15px;">
                                {{ flight['airlineName'] }}
                               </div>
                              </td>
                            </tr>
                          </tbody>
                        </table>    
                      </td>
                      <td width="4%">
                       <span></span>
                      </td>
                    </tr>

                    {% endfor %}  
                  </tbody>
                </table>
                <table class="margin-bottom" border="0" cellpadding="0" cellspacing="0" width="100%" height="35px" style="margin-bottom: 15px;">
                  <tbody>
                    <tr height="35px" colspan="5">
                       <td width="4%">
                            <span></span>
                       </td>                      
                       <td width="43%" align="left" class="text-cel2-col1 padding-top-23" style="font-size: 14px; font-family: lato; font-weight: normal; color: #54284E; padding-top: 23px;">
                          <div class="margin-left" style="margin-left: 15px;">
                          <span class="text-regular-14" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #808080;"> <br>
                          Política de cancelación no reembolsable</span>
                        </div>
                       </td>
                       <td width="12%" align="right" class="text-cel2-col1" style="font-size: 14px; font-family: lato; font-weight: normal; color: #54284E;">
                         <img width="60" height="70" align="center" src="http://navicu.com/images/flights/preciogarantizadomin.png">
                       </td>
                       <td width="32%" align="right" class="text-cel2-col1" style="font-size: 14px; font-family: lato; font-weight: normal; color: #54284E;">
                         <table>
                          <tbody>
                            {% if amountsInLocalCurrency %}
                            <tr>
                              <td align="right">
                                  <div class="margin-right" style="margin-right: 15px;">
                                     <span class="text-regular-14" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #808080;">Subtotal: {{ currencyLocalSimbol ~ " " ~ subTotalLocal |number_format(0, ',', '.') }}
                                     </span>
                                  </div>
                              </td>
                            </tr>
                            <tr>
                              <td align="right">
                                  <div class="margin-right" style="margin-right: 15px;">
                                     <span class="text-regular-14" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #808080;">Garantia de compra: {{ currencyLocalSimbol ~ " " ~ incrementGuaranteeLocal |number_format(0, ',', '.') }}
                                     </span>
                                  </div>
                              </td>
                            </tr>
                             <tr>
                              <td align="right">
                                  <div class="margin-right" style="margin-right: 15px;">
                                     <span class="text-regular-14" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #808080;">
                                      Gasto de emisión: {{ currencyLocalSimbol ~ " " ~ incrementExpensesLocal |number_format(0, ',', '.') }}
                                     </span>
                                  </div>
                              </td>
                            </tr>
                            <tr>
                              <td align="right">
                                  <div class="margin-right" style="margin-right: 15px;">
                                     <span class="text-regular-14" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #808080;">
                                      {% if discount > 0 %}
                                        Desc. navicu.com:
                                         {{ currencyLocalSimbol ~ " -" ~ discountLocal |number_format(0, ',', '.') }}

                                       {% else %}
                                         Desc. navicu.com: 
                                         {{ currencyLocalSimbol ~ " " ~ discountLocal |number_format(0, ',', '.') }}

                                       {% endif %}
                                     </span>
                                  </div>
                              </td>
                            </tr>

                            {% else %}

                             <tr>
                              <td align="right">
                                  <div class="margin-right" style="margin-right: 15px;">
                                     <span class="text-regular-14" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #808080;">Subtotal {{ CurrencyReservationSimbol ~ " " ~ subTotal |number_format(2, ',', '.') }}
                                     </span>
                                  </div>
                              </td>
                            </tr>
                             <tr>
                              <td align="right">
                                  <div class="margin-right" style="margin-right: 15px;">
                                     <span class="text-regular-14" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #808080;">Garantia de compra {{ CurrencyReservationSimbol ~ " " ~ incrementGuarantee |number_format(2, ',', '.') }}
                                     </span>
                                  </div>
                              </td>
                            </tr>
                            <tr>
                              <td align="right">
                                  <div class="margin-right" style="margin-right: 15px;">
                                     <span class="text-regular-14" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #808080;">
                                      Gasto de emisión {{ CurrencyReservationSimbol ~ " " ~ incrementExpenses |number_format(2, ',', '.') }}
                                     </span>
                                  </div>
                              </td>
                            </tr>
                            <tr>
                              <td align="right">
                                  <div class="margin-right" style="margin-right: 15px;">
                                     <span class="text-regular-14" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #808080;">
                                      {% if discount > 0 %}
                                        Desc. navicu.com:
                                         {{ currencyLocalSimbol ~ " -" ~ discountLocal |number_format(0, ',', '.') }}

                                       {% else %}
                                         Desc. navicu.com: 
                                         {{ currencyLocalSimbol ~ " " ~ discountLocal |number_format(0, ',', '.') }}

                                       {% endif %}

                                       {% if discount > 0 %}
                                        Desc. navicu.com 
                                        {{ CurrencyReservationSimbol ~ " -" ~ discount |number_format(2, ',', '.') }}
                                        {% else %}
                                        Desc. navicu.com 
                                        {{ CurrencyReservationSimbol ~ " " ~ discount |number_format(2, ',', '.') }}

                                        {% endif %}
                                     </span>
                                  </div>
                              </td>
                            </tr>
                            {% endif %}
                            {% if amountsInLocalCurrency %}
                            <tr>
                              <td align="right">
                                <div class="margin-right" style="margin-right: 15px;">
                                 <span class="text-regular-14" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #808080;">
                                 IVA {{ currencyLocalSimbol ~ " " ~ taxLocal |number_format(0, ',', '.') }}
                                  </span>
                                 </div>
                              </td>
                            </tr>
                            <tr>
                              <td align="right">
                                <div class="margin-right" style="margin-right: 15px;">
                                 <span class="text-regular-total" style="font-size: 14px; font-family: lato; font-weight: bold; color: #391860;">
                                  TOTAL {{ currencyLocalSimbol ~ " " ~ totalToPayLocal |number_format(0, ',', '.') }}
                                </span>
                                 </div>
                              </td>
                            </tr>
                            {% else %}

                            <tr>
                              <td align="right">
                                <div class="margin-right" style="margin-right: 15px;">
                                 <span class="text-regular-14" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #808080;">
                                  IVA {{ CurrencyReservationSimbol ~ " " ~ tax |number_format(2, ',', '.') }}
                                  </span>
                                 </div>
                              </td>
                            </tr>
                            <tr>
                              <td align="right">
                                <div class="margin-right" style="margin-right: 15px;">
                                 <span class="text-regular-total" style="font-size: 14px; font-family: lato; font-weight: bold; color: #391860;">
                                 TOTAL {{ CurrencyReservationSimbol ~ " " ~ totalToPay |number_format(2, ',', '.') }}
                                </span>
                                 </div>
                              </td>
                            </tr>
                            {% endif %}
                          </tbody>
                         </table> 
                       </td>
                       <td width="4%" rowspan="3">
                         <span></span>
                       </td>
                    </tr>       
