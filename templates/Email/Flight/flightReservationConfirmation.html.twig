            <!-- Detalles del pasajero -->
            <table bgcolor="#EFE4F5" border="0" cellpadding="0" cellspacing="0" width="100%" height="35px">
                  <tbody>
                    <tr>
                       <td width="4%">
                       <span></span>
                      </td>
                       <td width="4%">
                        <img width="15px" align="left" src="despiece/boleto.png"> 
                      </td>
                      <td width="35%" align="left" class="text-cel2-col1" style="font-size: 14px; font-family: lato; font-weight: normal; color: #54284E;">
                          Detalles del pasajero    
                      </td>
                      <td width="25%" align="left" class="text-cel2-col2" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #54284E;">
                      Equipaje
                      </td>
                      <td align="right">
                              <span class="space-15-right" style="margin-right: 15px; padding: 6px 6px;">
                                <span class="text-td-1" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #54284E;">ID </span>
                                <span class="text-td-2" style="font-size: 14px; font-family: lato; font-weight: bold; color: #54284E;">{{public_id}}</span>
                              </span>
                            </td>                 
                    </tr>
                  </tbody>
            </table>
            <table border="0" cellpadding="0" cellspacing="0" width="100%" height="35px">
                  <tbody>
                    {% for passenger in passengers %}
                     <tr bgcolor="{{ loop.index is divisible by(2) ? '#FDFBFE' : '#F0F0F0' }}" height="35px">
                       <td width="4%">
                       <span></span>
                       </td>
                     
                       <td width="39%" align="left">
                        <span class="text-details-col1" style="font-size: 14px; font-family: lato; font-weight: normal; color: #808080;">Nombres </span> 
                         <span class="text-details-col2" style="font-size: 14px; font-family: lato; font-weight: bold; color: #808080;">{{ passenger['firstName'] ~ " " ~ passenger['lastName'] }}
                         </span>   
                      </td>

                      <td width="25%" align="left" class="text-regular-12" style="font-size: 12px; font-family: lato; font-weight: lighter; color: #808080;">
                      1 bulto/2 piezas en bodega
                      </td>
                      {% if passenger.tickets|length < 2 %}
                      <td align="right" width="32%">
                              <span class="space-15-right" style="margin-right: 15px; padding: 6px 6px;">
                                <span class="text-td-1" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #54284E;">N° TKT - IDA</span>
                                <span class="text-td-2" style="font-size: 14px; font-family: lato; font-weight: bold; color: #54284E;">&nbsp;{{"  " ~ passenger.tickets[0].number }}
                                </span>
                              </span>
                       </td>
                       {% else %}
                        <td align="right" width="32%">
                              <span class="space-15-right" style="margin-right: 15px; padding: 6px 6px;">
                                <span class="text-td-1" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #54284E;">N° TKT - IDA</span>
                                <span class="text-td-2" style="font-size: 14px; font-family: lato; font-weight: bold; color: #54284E;">&nbsp;{{"  " ~ passenger.tickets[0].number }}
                                </span><br>
                                <span class="text-td-1" style="font-size: 14px; font-family: lato; font-weight: lighter; color: #54284E;">N° TKT - VUELTA</span>
                                <span class="text-td-2" style="font-size: 14px; font-family: lato; font-weight: bold; color: #54284E;">&nbsp{{ "  " ~ passenger.tickets[1].number }}
                                </span>
                              </span>
                       {% endif %}
                       </td>

                    </tr>
                    {% endfor %}
