                        <table role="presentation" cellpadding="0" cellspacing="0" style="background:#F2ECF8;" width="100%" border="0">
                            <tbody>
                            <tr>
                                <td style="word-wrap:break-word;font-size:0px;" align="left">
                                    <div style="cursor:auto;color:#000000;font-family:Ubuntu, Helvetica, Arial, sans-serif;font-size:13px;line-height:22px;text-align:left;">
                                        <p class="text-p" style="font-size: 16px; padding: 0 22px;margin-right: -11px; font-family: Lato; font-weight:300;text-align: inherit;color: #62269E ;">
                                            <strong>Compra de paquete exitosa:</strong>
                                        </p>

                                        <div class="text-p" style="font-size: 16px; padding: 0 22px;margin-right: -11px; font-family: Lato; font-weight:300;text-align: inherit;color: #62269E ;">
                                            <div>
                                                <div class="price LatoBlack">
                                                    <strong>Fechas:</strong>
                                                    {{ general.start }} -
                                                    {{ general.end }}
                                                </div>
                                                <div class="price LatoBlack">
                                                    <strong>Cantidad de paquetes:</strong>
                                                    {{ general.quantity }}
                                                </div>

                                                <div class="price LatoBlack">
                                                    <strong>Precio del paquete:</strong>
                                                    {{ package.symbol }}
                                                    {{ package.price }}
                                                </div>

                                                <div class="price LatoBlack">
                                                    <strong>Total pagado:</strong>
                                                    {{ package.symbol }}
                                                    {{ package.price * general.quantity }}
                                                </div>

                                                <div class="LatoBlack title">
                                                    <strong>Paquete para:</strong> {{ package.title }}
                                                </div>

                                                <p><strong>Informaci√≥n:</strong></p>
                                                <div class="LatoLightItalic">{{ package.subtitle }}</div>

                                                <div class="fs-14 mt-auto room">
                                                    <div class="LatoBold w-100">{{ package.room.time }}</div>
                                                    <div class="LatoLight w-100">{{ package.room.type }}</div>
                                                    <ul>
                                                        {% for detail in package.room.details %}
                                                            <li>
                                                                {{ detail }}
                                                            </li>
                                                        {% endfor %}
                                                    </ul>
                                                </div>

                                                {% if package.flight.type and package.flight.departure %}
                                                    <div class="LatoLight fs-14 mt-auto">
                                                        <span class="LatoBold">{{ package.flight.type }}</span> -
                                                        {{ package.flight.departure }}
                                                    </div>
                                                {% endif %}

                                                {% if package.footer %}
                                                    <div class="fs-14 mt-auto">
                                                        <div class="LatoBold">{{ package.footer.text }}</div>
                                                        <div class="LatoLight">{{ package.footer.subtext }}</div>
                                                    </div>
                                                {% endif %}
                                            </div>
                                        </div>

                                        <hr style="margin: 20px 0">

                                        <p class="text-p" style="font-size: 16px; padding: 0 22px;margin-right: -11px; font-family: Lato; font-weight:300;text-align: inherit;color: #62269E ;">
                                            <strong>Pasajeros:</strong>
                                        </p>

                                        {% for passenger in passengers %}

                                            <div class="text-p" style="font-size: 16px; padding: 0 22px;margin-right: -11px; font-family: Lato; font-weight:300;text-align: inherit;color: #62269E ;">
                                                <div>
                                                    <strong>Nombre y apellido:</strong>
                                                    {{ passenger.title }} {{ passenger.fullName }}
                                                </div>
                                                <div>
                                                    <strong>Pasaporte:</strong>
                                                    {{ passenger.documentNumber }}
                                                </div>
                                                <div>
                                                    <strong>Email:</strong>
                                                    {{ passenger.email }}
                                                </div>
                                                <div>
                                                    <strong>Telefono:</strong>
                                                    {{ passenger.codePhone }}{{ passenger.phone }}
                                                </div>
                                                <div>
                                                    <strong>Genero:</strong>
                                                    {{ passenger.gender == 'M' ? 'Masculino' : 'Femenino' }}
                                                </div>
                                                <div>
                                                    <strong>Fecha de nacimiento:</strong>
                                                    {{ passenger.birthDate }}
                                                </div>
                                            </div>
                                            <br>
                                        {% endfor %}

                                    </div>
                                </td>
                            </tr>
                            </tbody>
                        </table>
                    </div>

